{
  "name": "automat",
  "version": "0.0.0",
  "description": "State machine loveliness",
  "main": "automat-min.js",
  "module": "src/automat.js",
  "directories": {
    "src": "src",
    "lib": "lib"
  },
  "devDependencies": {
    "@zambezi/ez-build": "0.6.0",
    "babel-polyfill": "6.16.0",
    "source-map-support": "0.4.6"
  },
  "scripts": {
    "build": "ez-build --production @build.opts",
    "build:dev": "ez-build --interactive @build.opts",
    "test": "node -r babel-polyfill -r source-map-support/register lib/test/${TEST-http}.js",
    "test:dev": "npm run -s build:dev | while read -r line; do echo \"${line}\"; npm run -s test; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mstade/automat.git"
  },
  "keywords": [
    "DFA",
    "FSM"
  ],
  "author": "Marcus Stade <marcus@stade.se> (http://madebystade.se/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mstade/automat/issues"
  },
  "homepage": "https://github.com/mstade/automat#readme"
}
